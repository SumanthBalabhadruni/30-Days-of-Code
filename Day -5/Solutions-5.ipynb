      },
      "source": [
        "def maximuminthree(a,b,c):\n",
        "  if a>=b and a>=c:\n",
        "    return a\n",
        "  elif b>=c:\n",
        "    return b\n",
        "  else:\n",
        "    return c\n",
        "print(maximuminthree(1,3,2))\n",
        "  "
